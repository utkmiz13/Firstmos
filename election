#include <iostream>
using namespace std;
int main (){
    int age = 24;

    cout <<" enter your age "<<endl;
    cin >>age;
    if (age >= 18 )
    {
        cout <<" you can vote "<<endl;

    }else {
        cout << " bacha hai kya "<<endl;

    }
    if ( age >= 35){
        cout <<" you are aligible for president "<<endl;
    }
    return 0;
}
