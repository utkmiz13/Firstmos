#include <iostream>
using namespace std;
int main (){
    int age;
    string name;

    cout << " plz enter your age  :"<<endl;
    cin>>age;
    cout<<" your age is : " <<age<<endl;
    cout<< " plz enter your name "<<endl;
    cin>>name;
    cout<< " your name is :" <<name<<endl;
    if (age>=18){
        cout<< " welcome "<<name<<" you are alloed for vote "<<endl;

    }else{
        cout << " soory " <<name<<" you are not allowed for vote "<<endl;
    }
    if(age >=35){
        cout << " welcome " << name << "you are allowed for president "<<endl;
    }

    return 0;
}
